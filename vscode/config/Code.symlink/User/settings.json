{
    "editor.fontFamily": "SauceCodePro NF",
    "editor.fontSize": 13,
    "editor.formatOnType": true,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "all",
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 79,
    "explorer.confirmDelete": false,
    "explorer.openEditors.visible": 0,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue"
    ],
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 5000,
    "files.eol": "\n",
    "git.autofetch": true,
    "markdown.preview.fontSize": 13,
    "prettier.printWidth": 79,
    "prettier.disableLanguages": [],
    "python.linting.flake8Args": [],
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "python.venvPath": "C:\\Users\\odie\\Envs",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.external.windowsExec": "C:\\tools\\cmder\\Cmder.exe",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.rendererType": "dom",
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
    "vim.cmdLineInitialColon": true,
    "vim.enableNeovim": true,
    "vim.hlsearch": true,
    "vim.leader": ",",
    "vim.neovimPath": "C:\\Tools\\neovim\\Neovim\\bin\\nvim.exe",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [";"],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.showCommands",
                    "args": []
                }
            ]
        },
        {
            "before": ["leader", "leader"],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.files.save",
                    "args": []
                }
            ]
        },
        {
            "before": ["j"],
            "after": ["g", "j"]
        }
    ],
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "none",
    "editor.renderIndentGuides": false,
    "workbench.colorTheme": "Dark (Molokai)"
}