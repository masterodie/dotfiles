export PATH="$HOME/bin:$PATH"
export ENV_NAME=".venv"
export PAGER="vimpager"
if [ -b `which nvim` ]
then
	export EDITOR="nvim"
else
	export EDITOR="vim"
fi
export SUDO_EDITOR=$EDITOR
export GIT_EDITOR=$EDITOR
export BROWSER="firefox"
if (set -u; : $DEFAULT_USER) 2> /dev/null
then
else
        export DEFAULT_USER=$USER
fi
